{
  "buildCommand": "npm run vercel-build",
  "env": {
    "SOCKET_IO_ENABLED": "false",
    "NODE_ENV": "production"
  },
  "functions": {
    "src/app/api/**/*.js": {
      "maxDuration": 10
    }
  },
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}